{
  "name": "madnessenjin",
  "version": "2.0.0",
  "description": "app development assistant",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "npm run build:watch",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadnessLabs/MadnessEnjinCLI.git"
  },
  "author": "Madness Labs",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "enjin": "dist/index.js"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "browser-sync": "^2.18.13",
    "enjinrev": "^1.0.24",
    "fs-extra": "^1.0.0",
    "inquirer": "^0.11.0",
    "lodash": "^4.16.4",
    "nconf": "^0.8.4",
    "path": "^0.12.7",
    "replace": "^0.3.0",
    "rimraf": "^2.6.2",
    "slash": "^1.0.0",
    "suppose": "^0.6.1",
    "unirest": "^0.5.1",
    "yargs": "^4.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/node": "^10.3.0"
  }
}
